{
	"name": "vue-esm-loader-tests",
	"private": true,
	"type": "module",
	"scripts": {
		"test": "npm run test:3 && npm run test:2.7 && npm run test:2.6",
		"test:all": "npm run mocha -w core",
		"pretest:3": "npm install --save-dev vue@3 @vue/test-utils@2",
		"test:3": "npm run test:all",
		"posttest:3": "npm uninstall vue @vue/test-utils",
		"pretest:2.7": "npm install --save-dev vue@2.7 @vue/test-utils@1.1.1",
		"test:2.7": "npm run test:all",
		"posttest:2.7": "npm uninstall vue @vue/test-utils",
		"pretest:2.6": "npm install --save-dev vue@2.6 @vue/test-utils@1.1.1 vue-template-compiler@2.6 @vue/component-compiler-utils",
		"test:2.6": "npm run test:all",
		"posttest:2.6": "npm uninstall vue @vue/test-utils vue-template-compiler @vue/component-compiler-utils"
	}
}
